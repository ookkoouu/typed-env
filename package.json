{
  "name": "@okou/typed-env",
  "version": "0.1.0",
  "description": "Simple typed-parser of `process.env`.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "build": "tsc -p .",
    "build:clean": "run-s clean build",
    "build:check": "run-s 'build --noEmit'",
    "lint": "eslint --ext .ts src",
    "lint:fix": "run-s 'lint' --fix",
    "format": "prettier -w {src,test}/**/*.ts",
    "fix": "run-s format lint:fix",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ookkoouu/typed-env.git"
  },
  "keywords": [],
  "author": "okou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ookkoouu/typed-env/issues"
  },
  "homepage": "https://github.com/ookkoouu/typed-env#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
